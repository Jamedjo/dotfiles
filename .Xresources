URxvt.perl-ext-common: default,keyboard-select,font-size
URxvt.keysym.C-equal: font-size:increase
URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-f: perl:keyboard-select:search
URxvt.keysym.Control-s: perl:keyboard-select:activate
URxvt.keyboard-select.clipboard: true
URxvt.iso14755: false
URxvt.iso14755_52: false

!! Fonts
*.font: xft:Monospace:pixelsize=16:antialias=true:xft:monospace
*.boldFont: xft:Monospace:pixelsize=16:weight=bold:antialias=true:xft:monospace
URxvt*letterSpace: -1

!! Colorscheme
  urxvt*depth: 32
  urxvt*background: [96]#181818
  *.background: #121222
  *.foreground: #FFFFFF
! *.foreground: #2E2C5F
  *.cursorColor: #8B8B8B
! black
  *.color0: #212121
  *.color8: #78332A
! red
  *.color1: #AF743A
  *.color9: #FF2177
! green
  *.color2: #444433
  *.color10: #BABA58
! yellow
  *.color3: #99997C
  *.color11: #FECC77
! blue
  *.color4: #AA9987
  !*.color4: #8794A9
  *.color12: #77C3E2
! magenta
  *.color5: #CCCCCC
  *.color13: #B291DD
! cyan
  *.color6: #ECECEC
  *.color14: #98DDDB
! white
  *.color7: #FFFFFF
  *.color15: #DBAB83

!! Navigation
! Scroll one line up
URxvt.keysym.Shift-Up: command:\033]720;1\007
! Scroll one line down
URxvt.keysym.Shift-Down: command:\033]721;1\007

!! URxvt config
URxvt.saveLines: 1000000
URxvt.scrollBar: false
